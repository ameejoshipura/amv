        <activity
            android:name=".vectorProductPolar"
            android:label="@string/title_activity_vector_product_polar" >
        </activity>
        <activity
            android:name=".resultScalarProduct"
            android:label="@string/title_activity_result_scalar_product" >
        </activity>
        <activity
            android:name=".resultVectorProduct"
            android:label="@string/title_activity_result_vector_product" >
        </activity>
        <activity
            android:name=".resultTwoVectors"
            android:label="@string/title_activity_result_two_vectors" >
        </activity>
        <activity
            android:name=".resultThreeVectors"
            android:label="@string/title_activity_result_three_vectors" >
        </activity>
        <activity
