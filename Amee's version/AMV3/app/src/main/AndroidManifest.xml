        <activity
            android:name=".chooseOperationPolar"
            android:label="@string/title_activity_choose_operation_polar" >
        </activity>
        <activity
            android:name=".addTwoVectors"
            android:label="@string/title_activity_add_two_vectors" >
        </activity>
        <activity
            android:name=".addThreeVectors"
            android:label="@string/title_activity_add_three_vectors" >
        </activity>
        <activity
            android:name=".scalarProduct"
            android:label="@string/title_activity_scalar_product" >
